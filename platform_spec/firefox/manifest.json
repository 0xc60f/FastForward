{
	"manifest_version": 3,
	"icons": {
		"48": "icon/48.png",
		"128": "icon/128.png",
		"150": "icon/150.png",
		"176": "icon/176.png",
		"512": "icon/512.png"
	},
	"default_locale": "en",
	"name": "FastForward",
	"description": "__MSG_appDesc__",
	"homepage_url": "https://fastforward.team",
	"version": "13.15.2",
	"author": "FastForward Team",
	"browser_specific_settings": {
		"gecko": {
		  "id": "addon@fastforward.team",
		  "strict_min_version": "109.0"
		}
	},
	"permissions": [
		"alarms",
		"storage",
		"webNavigation"
	],
	"options_ui": {
		"page": "html/options.html",
		"open_in_tab": true
	},
	"background": {
		"scripts": ["background.js"]
	},
	"action": {},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content_script.js"
			],
			"run_at": "document_start"
		}
	],
	
	"web_accessible_resources": [{
		"resources": [
		"html/before-navigate.html",
		"html/blocked.html",
		"html/crowd-bypassed.html",
		"html/options.html",
		"icon/48.png",
		"injection_script.js",
		"bypasses/*",
		"helpers/*"],
		"matches": ["<all_urls>"]
}]
}
